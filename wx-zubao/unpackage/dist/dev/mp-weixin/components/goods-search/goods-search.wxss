.content_box {
  width: 100%;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 20rpx 50rpx;
}
.content_box .box {
    width: 100%;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    padding-left: 30rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    border-radius: 50rpx;
    border: 4rpx solid #ea986c;
}
.content_box .box input {
      width: 80%;
      height: 70rpx;
      font-size: 30rpx;
      color: #333333;
}
.content_box .box button {
      width: 20%;
      line-height: 70rpx;
      background: none;
      text-align: center;
      height: 70rpx;
}
.content_box .box button text {
        font-size: 50rpx;
        color: #d0d0d0;
}
.content_box .box button:after {
      border: none;
}

